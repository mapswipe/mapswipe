{
  "name": "mapswipe",
  "version": "1.3.27",
  "private": true,
  "scripts": {
    "buildDev": "cd android && ./gradlew assembleDevRelease && cd ..",
    "buildProd": "cd android && ./gradlew assembleProductionRelease && cd ..",
    "reduxDebugger": "remotedev-debugger --hostname localhost --port 5678 --injectserver",
    "lint": "node_modules/eslint_d/bin/eslint.js src __tests__ --cache",
    "postinstall": "node scripts/postinstall.js",
    "runAndroid": "cd android && ./gradlew installDevDebug && adb -d shell am start -n org.missingmaps.mapswipe.dev/org.missingmaps.mapswipe.MainActivity ; cd ..",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --forceExit",
    "preversion": "yarn lint && yarn flow && yarn test",
    "postversion": "git push --follow-tags && echo \"Successfully released version $npm_package_version!\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@mapbox/tilebelt": "^1.0.1",
    "apsl-react-native-button": "^3.1.1",
    "i18next": "^13.1.0",
    "lodash": "^4.17.11",
    "react": "16.6.3",
    "react-i18next": "^9.0.2",
    "react-native": "0.58.0",
    "react-native-animatable": "^1.3.0",
    "react-native-device-info": "^0.24.0",
    "react-native-firebase": "^5.1.0",
    "react-native-linear-gradient": "^2.4.2",
    "react-native-message-bar": "^2.0.10",
    "react-native-modalbox": "^1.6.0",
    "react-native-progress": "^3.5.0",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-sentry": "^0.41.1",
    "react-native-simple-markdown": "^1.1.0",
    "react-native-simple-store": "^1.3.0",
    "react-native-splash-screen": "^3.1.1",
    "react-native-swiper": "1.5.13",
    "react-navigation": "^2.18.1",
    "react-redux": "^5.1.0",
    "react-redux-firebase": "^2.2.6",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "24.7.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint_d": "^7.3.0",
    "firebase-mock": "^2.2.10",
    "flow-bin": "^0.85.0",
    "jest": "24.7.1",
    "metro-react-native-babel-preset": "0.53.1",
    "native-testing-library": "^3.1.2",
    "react-test-renderer": "16.6.3",
    "remote-redux-devtools": "^0.5.16",
    "remotedev-rn-debugger": "^0.8.4"
  },
  "jest": {
    "preset": "native-testing-library",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__tests__/fixtures"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|apsl-react-native-button|react-native-firebase|react-native-message-bar|react-native-modalbox|react-native-linear-gradient)/)"
    ]
  }
}
